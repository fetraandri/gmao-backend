{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/equipments/(.*)",
        "dest": "/api/equipments.js"
      },
      {
        "src": "/api/interventions/(.*)",
        "dest": "/api/interventions.js"
      },
      {
        "src": "/api/maintenance/(.*)",
        "dest": "/api/maintenance.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://gmao-app.vercel.app"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE, OPTIONS"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type, Authorization"
          },
          {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
          }
        ]
      }
    ]
  }